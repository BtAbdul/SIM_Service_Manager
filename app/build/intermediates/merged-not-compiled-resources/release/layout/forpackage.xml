<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear157"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#EEEEEE"
		android:orientation="vertical">
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cardview6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="5dp"
			android:layout_marginTop="10dp"
			android:layout_marginRight="5dp"
			android:layout_marginBottom="10dp"
			android:padding="2dp"
			android:background="#CFD8DC"
			android:orientation="horizontal"
			app:cardElevation="5dp"
			app:cardCornerRadius="7dp"
			app:cardBackgroundColor="#CFD8DC"
			style="@style/Widget.Material3.CardView.Elevated">
			<LinearLayout
				android:id="@+id/linear148"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#CFD8DC"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear149"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear153"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="#4A6572"
						android:orientation="vertical"
						android:layout_weight="1">
						<TextView
							android:id="@+id/title"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="55dp"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="100 ام بی روزانه"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#FFFFFF"
							android:layout_gravity="right"
							android:fontFamily="@font/titar" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear150"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#BDBDBD"
					android:orientation="horizontal" />
				<TextView
					android:id="@+id/price"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:paddingLeft="5dp"
					android:paddingTop="2dp"
					android:paddingRight="5dp"
					android:paddingBottom="2dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="15 افغانی"
					android:textSize="18sp"
					android:textStyle="bold"
					android:textColor="#000000"
					android:layout_gravity="center_horizontal|center_vertical"
					android:elevation="5dp"
					android:fontFamily="@font/titar" />
				<TextView
					android:id="@+id/info"
					android:focusable="false"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="13dp"
					android:paddingTop="5dp"
					android:paddingRight="13dp"
					android:paddingBottom="10dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="د ام ټي ان مومو له لارې نورو ته د کرېډټ پیرلو لپاره، #222* ډایل کړئ. 2 غوراوی وټاکئ، 1 بیا 2 وټاکئ، د کرېډټ اندازه او د پام وړ شمېره مو ولیکئ. د تایید لپاره، خپل پین کوډ ولیکئ. "
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#344955"
					android:fontFamily="@font/titar" />
				<LinearLayout
					android:id="@+id/linear151"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#BDBDBD"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/linear154"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="left"
					android:orientation="horizontal"
					android:layout_gravity="left"
					android:layout_weight="1">
					<com.google.android.material.button.MaterialButton
						android:id="@+id/buttonA"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_margin="5dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="@string/buttonA"
						android:textSize="12sp"
						android:textStyle="bold"
						android:textColor="#FFFFFF"
						android:textAppearance="@style/TextAppearance.MaterialComponents.Button"
						app:backgroundTint="#03C03C"
						app:cornerRadius="8dp"
						style="Widget.MaterialComponents.Button.Icon"
						app:icon="@drawable/active"
						app:iconTint="#ffffff"
						android:fontFamily="@font/titar" />
					<LinearLayout
						android:id="@+id/linear155"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:orientation="vertical"
						android:layout_weight="1" />
					<com.google.android.material.button.MaterialButton
						android:id="@+id/buttonD"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_margin="5dp"
						android:padding="8dp"
						android:gravity="right|center_vertical"
						android:text="@string/buttonD"
						android:textSize="12sp"
						android:textStyle="bold"
						android:textColor="#FFFFFF"
						android:layout_gravity="right|center_vertical"
						android:textAppearance="@style/TextAppearance.MaterialComponents.Button"
						app:backgroundTint="#FF033E"
						app:cornerRadius="8dp"
						style="Widget.MaterialComponents.Button.Icon"
						app:icon="@drawable/dactive"
						app:iconTint="#ffffff"
						android:fontFamily="@font/titar" />
				</LinearLayout>
			</LinearLayout>
		</com.google.android.material.card.MaterialCardView>
	</LinearLayout>
</LinearLayout>
